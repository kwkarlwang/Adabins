{
    "name": "vit_hybrid_reassemble_conv",
    "version": "mse",
    "gpus": 1,
    "experiment": {
        "task": "depth",
        "learning_rate": 3e-4,
        "epochs": 120,
        "loss": "l2"
    },
    "model": {
        "name": "transformer reassemble",
        "encoder": {
            "name": "hybrid",
            "hooks": [
                11
            ],
            "dropout": 0.1
        },
        "reassemble": {
            "num_class": 41,
            "img_size": 384
        }
    },
    "dataset": {
        "name": "nyu",
        "batch_size": 4,
        "num_workers": 8,
        "use_transform": false,
        "img_size": [
            480,
            640
        ]
    }
}